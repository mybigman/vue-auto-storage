{
  "name": "vue-auto-storage",
  "version": "2.0.0",
  "description": "An automatic storage plugin for Vue2, persist the data with localStorage.",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "poi dev",
    "build": "poi build",
    "serve": "vue-cli-service serve",
    "build:demo": "rimraf docs && vue-cli-service build",
    "test": "cross-env NODE_ENV=test jest",
    "deploy": "npm run test && npm run build && npm run build:demo"
  },
  "files": [
    "README.md",
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zh-rocco/vue-auto-storage.git"
  },
  "keywords": [
    "vue-auto-storage",
    "auto-storage",
    "persistence",
    "persistent-storage",
    "vue-plugin",
    "vue"
  ],
  "author": {
    "name": "Rocco Mormont",
    "email": "rocco.mormont@gmail.com",
    "url": "https://zh-rocco.github.io/"
  },
  "bugs": {
    "url": "https://github.com/zh-rocco/vue-auto-storage/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@poi/plugin-ts-check": "^11.0.0-beta.4",
    "@vue/cli-service": "^3.1.3",
    "poi": "^11.0.0-beta.4",
    "typescript": "^3.1.6",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "jest": {
    "clearMocks": true,
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "verbose": true
  }
}
